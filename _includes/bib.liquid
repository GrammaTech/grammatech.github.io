<!DOCTYPE html>
<html>
  <head>
    {% include head %}
    <link rel="stylesheet" href="https://cdn.plyr.io/3.6.7/plyr.css" />
  </head>
  <body style="color: #bdc1c6; background: #202124;">
    <header>
      {% include g-link %}
    </header>
    <div class="w3-content w3-padding-large w3-margin-top gt-min-left-margin">
      <h1>{{ TITLE }}</h1>
      <div class="w3-container">
      {{ AUTHOR }}
      <dl>
      {% if JOURNAL %}
        <dt>Journal</dt><dd>{{ JOURNAL }}</dd>
      {% endif %}
      {% if BOOKTITLE %}
        <dt>Booktitle</dt><dd>{{ BOOKTITLE }}</dd>
      {% endif %}
      {% if VOLUME %}
        <dt>Volume</dt><dd>{{ VOLUME }}</dd>
      {% endif %}
      {% if YEAR %}
        <dt>Year</dt><dd>{{ YEAR }}</dd>
      {% endif %}
      {% if URL %}
        <dt><a href="{{ URL }}">link</a></dt>
      {% endif %}
      {% if BIBURL %}
        <dt><a href="{{ BIBURL }}">bibtex</a></dt>
      {% endif %}
      </dl>
      </div>
    {% include support %}
    {% include bottom %}
</body>
{% include bottom-js %}
</html>
